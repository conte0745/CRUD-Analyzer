<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.jar.JarMapper">
    <select id="findFromJar" resultType="String">
        SELECT name FROM jar_table WHERE id = #{id}
    </select>
    <insert id="insertToJar" parameterType="String">
        INSERT INTO jar_table (name) VALUES (#{name})
    </insert>
</mapper>
